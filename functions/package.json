{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint . --fix",
    "serve:production": "firebase emulators:start --only functions --project gatherlynx-57286",
    "serve:staging": "firebase emulators:start --only functions --project gatherlynxstaging",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy:production": "firebase deploy --only functions --project gatherlynx-57286",
    "deploy:staging": "firebase deploy --only functions --project gatherlynxstaging",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "main": "index.js",
  "dependencies": {
    "cors": "^2.8.5",
    "eslint-config-next": "^15.5.2",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1",
    "next": "^15.5.2"
  },
  "devDependencies": {
    "eslint": "8.56.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.1.0",
    "ts-migrate": "^0.1.35"
  },
  "private": true
}
