{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint . --fix",
    "serve:main": "npm run build && firebase emulators:start --only functions --project gatherlynx-57286",
    "serve:dev": "npm run build && firebase emulators:start --only functions --project gatherlynxstaging",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy:main": "firebase deploy --only functions --project gatherlynx-57286",
    "deploy:dev": "firebase deploy --only functions --project gatherlynxstaging",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "20"
  },
  "main": "lib/index.js",
  "dependencies": {
    "cors": "^2.8.5",
    "eslint-config-next": "^15.5.2",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1",
    "next": "^15.5.2"
  },
  "devDependencies": {
    "eslint": "8.56.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.1.0",
    "ts-migrate": "^0.1.35",
    "typescript": "^5.7.3"
  },
  "private": true
}
